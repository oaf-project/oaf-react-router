  },
  "devDependencies": {
    "@stryker-mutator/core": "^5.3.0",
    "@stryker-mutator/jest-runner": "^5.3.0",
    "@stryker-mutator/typescript-checker": "^5.3.0",
    "@types/history": "^4.7.9",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "codecov": "^3.8.3",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-typed-fp": "^1.5.0",
    "eslint-plugin-functional": "^3.7.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sonarjs": "^0.10.0",
    "eslint-plugin-total-functions": "^4.9.0",
    "history": "^4.10.1",
    "jest": "^27.1.0",
    "prettier": "^2.3.2",
    "total-functions": "^3.0.0",
    "ts-jest": "^27.0.5",
    "type-coverage": "^2.18.1",
    "typedoc": "^0.22.0",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "oaf-routing": "^4.0.1"
  },
  "peerDependencies": {
    "history": "^4.10.1"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write '{src,test}/**/*.{ts,tsx}'",
    "documentation": "typedoc --excludeNotExported --excludePrivate --out docs src && cp docs_config.yml ./docs/_config.yml",
    "release": "yarn build && yarn lint && yarn type-coverage && yarn format-lint && yarn publish && yarn documentation",
    "test": "jest"
  },
  "prettier": {
    "trailingComma": "all"
  },
  "typeCoverage": {
    "atLeast": 100,
    "ignoreCatch": false,
    "strict": true,
    "detail": true
  }
}
